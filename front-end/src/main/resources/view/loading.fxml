<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.ProgressBar?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.VBox?>

<VBox alignment="TOP_CENTER" spacing="10.0" style="-fx-background-color: #9AE1EF;" stylesheets="@../style/style.css" xmlns="http://javafx.com/javafx/15.0.1" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <ImageView fitHeight="150.0" fitWidth="200.0" pickOnBounds="true" preserveRatio="true">
         <VBox.margin>
            <Insets top="10.0" />
         </VBox.margin>
         <image>
            <Image url="@../image/wait.gif" />
         </image>
      </ImageView>
      <ProgressBar fx:id="progressBar" prefHeight="5.0" prefWidth="200.0" />
   </children>
   <padding>
      <Insets bottom="10.0" />
   </padding>
</VBox>
